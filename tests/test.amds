decl sqrt : (number: f32) -> f32
{
	let x := number;
	let y: f32 = 1.0;
	let e: f32 = 0.000001;

	while x - y > e {
		x = (x + y) / 2;
		y = number / x;
	}

	return x;
}

// Implicitly returns nothing (i.e. decl main : () -> nothing { })
decl main : ()
{
	let value: f32 = 25.0;
	let s_value := sqrt(value);

	println("sqrt(%f) = %f", value, s_value);
}
